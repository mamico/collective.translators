<?xml version="1.0" encoding="utf-8"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        name="portal_controlpanel"
        i18n:domain="collective.translators"
>

  <configlet action_id="deepl-translator-controlpanel"
             appId="deepl-translator-controlpanel"
             category="Products"
             title="Deepl Settings"
             url_expr="string:${portal_url}/@@deepl-translator-controlpanel"
             visible="True"
             i18n:attributes="title"
  >
    <permission>Manage portal</permission>
  </configlet>

  <configlet action_id="aws-translator-controlpanel"
             appId="aws-translator-controlpanel"
             category="Products"
             title="AWS Translate Settings"
             url_expr="string:${portal_url}/@@aws-translator-controlpanel"
             visible="True"
             i18n:attributes="title"
  >
    <permission>Manage portal</permission>
  </configlet>

  <configlet action_id="libretranslate-translator-controlpanel"
             appId="libretranslate-translator-controlpanel"
             category="Products"
             title="LibreTranslate Settings"
             url_expr="string:${portal_url}/@@libretranslate-translator-controlpanel"
             visible="True"
             i18n:attributes="title"
  >
    <permission>Manage portal</permission>
  </configlet>

  <configlet action_id="deepseek-translator-controlpanel"
             appId="deepseek-translator-controlpanel"
             category="Products"
             title="DeepSeek Settings"
             url_expr="string:${portal_url}/@@deepseek-translator-controlpanel"
             visible="True"
             i18n:attributes="title"
  >
    <permission>Manage portal</permission>
  </configlet>
</object>
