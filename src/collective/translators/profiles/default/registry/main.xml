<?xml version="1.0" encoding="utf-8"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="collective.translators"
>

  <!-- Deepl Control Panel -->
  <records interface="collective.translators.interfaces.IDeeplControlPanel"
           >
    <value key="enabled">False</value>
    <value key="order">30</value>
    <value key="source_languages"></value>
    <value key="target_languages"></value>
    <value key="api_key">YOUR_DEEPL_API_KEY</value>
  </records>

  <!-- Amazon Translate Control Panel -->
  <records interface="collective.translators.interfaces.IAmazonTranslateControlPanel"
           >
    <value key="enabled">False</value>
    <value key="order">30</value>
    <value key="source_languages"></value>
    <value key="target_languages"></value>
    <value key="access_key">YOUR_AMAZON_ACCESS_API_KEY</value>
    <value key="secret_key">YOUR_AMAZON_SECRET_API_KEY</value>
    <value key="region_name">eu-west-3</value>
  </records>

  <!-- LibreTranslate Control Panel -->
  <records interface="collective.translators.interfaces.ILibreTranslateControlPanel"
           >
    <value key="enabled">False</value>
    <value key="order">30</value>
    <value key="source_languages"></value>
    <value key="target_languages"></value>
    <value key="api_key">YOUR_LIBRETRANSLATE_API_KEY</value>
  </records>

</registry>
